{
    "name": "meme-tools",
    "version": "1.0.0",
    "description": "Meme Tools",
    "scripts": {
        "prepare": "husky",
        "format": "prettier --check .",
        "format:fix": "prettier --write .",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "clean": "rm -rf dist/* logs/* storages/*",
        "start": "yarn distribute-funds && yarn create-mint && yarn open-raydium-pool && yarn burn-raydium-pool-liquidity:view",
        "start:clean": "yarn clean && yarn start",
        "finish": "yarn close-raydium-pool && yarn collect-funds:view",
        "distribute-funds": "ts-node src/tasks/distribute-funds.ts",
        "distribute-funds:view": "yarn distribute-funds && yarn get-funds",
        "distribute-funds:replay": "LOGGER_NAME=distribute-funds ts-node src/tasks/replay-logs.ts",
        "create-mint": "ts-node src/tasks/create-mint.ts",
        "create-mint:view": "yarn create-mint && yarn get-mint && yarn get-funds",
        "create-mint:replay": "LOGGER_NAME=create-mint ts-node src/tasks/replay-logs.ts",
        "open-raydium-pool": "ts-node src/tasks/open-raydium-pool.ts",
        "open-raydium-pool:view": "yarn open-raydium-pool && yarn get-funds",
        "open-raydium-pool:replay": "LOGGER_NAME=open-raydium-pool ts-node src/tasks/replay-logs.ts",
        "add-raydium-pool-liquidity": "ts-node src/tasks/add-raydium-pool-liquidity.ts",
        "add-raydium-pool-liquidity:view": "yarn add-raydium-pool-liquidity && yarn get-funds",
        "add-raydium-pool-liquidity:replay": "LOGGER_NAME=add-raydium-pool-liquidity ts-node src/tasks/replay-logs.ts",
        "burn-raydium-pool-liquidity": "ts-node src/tasks/burn-raydium-pool-liquidity.ts",
        "burn-raydium-pool-liquidity:view": "yarn burn-raydium-pool-liquidity && yarn get-funds",
        "burn-raydium-pool-liquidity:replay": "LOGGER_NAME=burn-raydium-pool-liquidity ts-node src/tasks/replay-logs.ts",
        "trade-raydium-pool-volume": "POOL_TRADING_MODE=volume ts-node src/tasks/trade-raydium-pool.ts",
        "trade-raydium-pool-volume:view": "yarn trade-raydium-pool-volume && yarn get-funds",
        "trade-raydium-pool-pump": "POOL_TRADING_MODE=pump ts-node src/tasks/trade-raydium-pool.ts",
        "trade-raydium-pool-pump:view": "yarn trade-raydium-pool-pump && yarn get-funds",
        "trade-raydium-pool-dump": "POOL_TRADING_MODE=dump ts-node src/tasks/trade-raydium-pool.ts",
        "trade-raydium-pool-dump:view": "yarn trade-raydium-pool-dump && yarn get-funds",
        "trade-raydium-pool:replay": "LOGGER_NAME=trade-raydium-pool ts-node src/tasks/replay-logs.ts",
        "close-raydium-pool": "ts-node src/tasks/close-raydium-pool.ts",
        "close-raydium-pool:view": "yarn close-raydium-pool && yarn get-funds",
        "close-raydium-pool:replay": "LOGGER_NAME=close-raydium-pool ts-node src/tasks/replay-logs.ts",
        "collect-funds": "ts-node src/tasks/collect-funds.ts",
        "collect-funds:view": "yarn collect-funds && yarn get-funds",
        "collect-funds:replay": "LOGGER_NAME=collect-funds ts-node src/tasks/replay-logs.ts",
        "get-accounts": "ts-node src/tasks/get-accounts.ts",
        "get-funds": "ts-node src/tasks/get-funds.ts",
        "get-mint": "ts-node src/tasks/get-mint.ts",
        "get-raydium-pool": "ts-node src/tasks/get-raydium-pool.ts"
    },
    "keywords": [],
    "author": "EverCoinX <2901744+evercoinx@users.noreply.github.com>",
    "license": "UNLICENSED",
    "private": true,
    "type": "commonjs",
    "engines": {
        "node": ">=22"
    },
    "dependencies": {
        "@raydium-io/raydium-sdk-v2": "^0.1.115-alpha",
        "@solana/spl-token": "^0.4.9",
        "@solana/web3.js": "^1.98.0",
        "arweave": "^1.15.5",
        "bs58": "^6.0.0",
        "chalk": "^5.4.1",
        "decimal.js": "^10.5.0",
        "dotenv": "^16.4.7",
        "fastestsmallesttextencoderdecoder": "^1.0.22",
        "flat-cache": "^6.1.5",
        "got": "^14.4.6",
        "helius-sdk": "^1.4.2",
        "pinata-web3": "^0.5.4",
        "pino": "^9.6.0",
        "pino-pretty": "^13.0.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.18.0",
        "@tsconfig/node20": "^20.1.4",
        "@types/big.js": "^6.2.2",
        "@types/bn.js": "^5.1.6",
        "@types/decimal.js": "^7.4.3",
        "@types/minimist": "^1.2.5",
        "@types/node": "^22.10.7",
        "eslint": "^9.18.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-prettier": "^5.2.3",
        "husky": "^9.1.7",
        "joi": "^17.13.3",
        "prettier": "^3.4.2",
        "source-map-support": "^0.5.21",
        "ts-node": "^10.9.2",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.21.0"
    }
}
