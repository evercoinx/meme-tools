{
    "name": "meme-tools",
    "version": "1.0.0",
    "description": "Meme Tools",
    "scripts": {
        "prepare": "husky && yarn set-env:dev",
        "set-env:dev": "yarn config set initFields --home --json '{\"meme-tools\": {\"env\": \"development\"}}'",
        "set-env:test": "yarn config set initFields --home --json '{\"meme-tools\": {\"env\": \"test\"}}'",
        "set-env:prod": "yarn config set initFields --home --json '{\"meme-tools\": {\"env\": \"production\"}}'",
        "get-env": "yarn config get 'initFields[\"meme-tools\"].env'",
        "format": "prettier --check .",
        "format:fix": "prettier --write .",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "test:raydium": "yarn set-env:test && yarn start:raydium && yarn add-liquidity:raydium && yarn trade:raydium && yarn buy-mint:whale1 && yarn sell-mint:whale1 && yarn finish:raydium && yarn set-env:dev",
        "start:raydium": "yarn get-config && yarn cleanup && yarn transfer-funds && yarn distribute-funds && yarn create-mint && yarn open-pool:raydium && yarn get-pool:raydium && yarn get-funds:main",
        "finish:raydium": "yarn close-pool:raydium && yarn collect-funds && yarn remove-liquidity:raydium && yarn get-funds",
        "cleanup": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/cleanup.ts",
        "distribute-funds": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/distribute-funds.ts",
        "distribute-funds:replay": "NODE_ENV=$(yarn get-env) LOGGER_NAME=distribute-funds ts-node -r ./src/init.ts src/tasks/replay-logs.ts",
        "distribute-funds:dry-run": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/distribute-funds.ts --dry-run",
        "create-mint": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/create-mint.ts",
        "create-mint:replay": "NODE_ENV=$(yarn get-env) LOGGER_NAME=create-mint ts-node -r ./src/init.ts src/tasks/replay-logs.ts",
        "open-pool:raydium": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/raydium/open-pool.ts",
        "open-pool:raydium:replay": "NODE_ENV=$(yarn get-env) LOGGER_NAME=open-pool-raydium ts-node -r ./src/init.ts src/tasks/replay-logs.ts",
        "add-liquidity:raydium": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/raydium/add-liquidity.ts",
        "add-liquidity:raydium:replay": "NODE_ENV=$(yarn get-env) LOGGER_NAME=add-liquidity-raydium ts-node -r ./src/init.ts src/tasks/replay-logs.ts",
        "remove-liquidity:raydium": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/raydium/remove-liquidity.ts",
        "remove-liquidity:raydium:replay": "NODE_ENV=$(yarn get-env) LOGGER_NAME=remove-liquidity -raydium ts-node -r ./src/init.ts src/tasks/replay-logs.ts",
        "burn-liquidity:raydium": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/raydium/burn-liquidity.ts",
        "burn-liquidity:raydium:replay": "NODE_ENV=$(yarn get-env) LOGGER_NAME=burn-liquidity-raydium ts-node -r ./src/init.ts src/tasks/replay-logs.ts",
        "lock-liquidity:raydium": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/raydium/lock-liquidity.ts",
        "lock-liquidity:raydium:replay": "NODE_ENV=$(yarn get-env) LOGGER_NAME=lock-liquidity-raydium ts-node -r ./src/init.ts src/tasks/replay-logs.ts",
        "trade:raydium": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/raydium/trade.ts",
        "trade:raydium:replay": "NODE_ENV=$(yarn get-env) LOGGER_NAME=trade-raydium ts-node -r ./src/init.ts src/tasks/replay-logs.ts",
        "buy-mint:whale1": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/raydium/swap-whale-mint.ts --index 0 --swap-type buy",
        "buy-mint:whale2": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/raydium/swap-whale-mint.ts --index 1 --swap-type buy",
        "buy-mint:whale3": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/raydium/swap-whale-mint.ts --index 2 --swap-type buy",
        "buy-mint:whale4": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/raydium/swap-whale-mint.ts --index 3 --swap-type buy",
        "buy-mint:whale5": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/raydium/swap-whale-mint.ts --index 4 --swap-type buy",
        "sell-mint:whale1": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/raydium/swap-whale-mint.ts --index 0 --swap-type sell",
        "sell-mint:whale2": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/raydium/swap-whale-mint.ts --index 1 --swap-type sell",
        "sell-mint:whale3": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/raydium/swap-whale-mint.ts --index 2 --swap-type sell",
        "sell-mint:whale4": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/raydium/swap-whale-mint.ts --index 3 --swap-type sell",
        "sell-mint:whale5": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/raydium/swap-whale-mint.ts --index 4 --swap-type sell",
        "close-pool:raydium": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/raydium/close-pool.ts",
        "close-pool:raydium:replay": "NODE_ENV=$(yarn get-env) LOGGER_NAME=close-pool-raydium ts-node -r ./src/init.ts src/tasks/replay-logs.ts",
        "collect-fees:raydium": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/raydium/collect-fees.ts",
        "collect-fees:raydium:replay": "NODE_ENV=$(yarn get-env) LOGGER_NAME=collect-fees-raydium ts-node -r ./src/init.ts src/tasks/replay-logs.ts",
        "collect-funds": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/collect-funds.ts",
        "collect-funds:replay": "NODE_ENV=$(yarn get-env) LOGGER_NAME=collect-funds ts-node -r ./src/init.ts src/tasks/replay-logs.ts",
        "transfer-funds": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/transfer-funds.ts",
        "transfer-funds:replay": "NODE_ENV=$(yarn get-env) LOGGER_NAME=transfer-funds ts-node -r ./src/init.ts src/tasks/replay-logs.ts",
        "get-config": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/get-config.ts",
        "get-accounts": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/get-accounts.ts",
        "get-accounts:main": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/get-accounts.ts --mode main",
        "get-accounts:sniper": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/get-accounts.ts --mode sniper",
        "get-accounts:trader": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/get-accounts.ts --mode trader",
        "get-accounts:whale": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/get-accounts.ts --mode whale",
        "get-funds": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/get-funds.ts",
        "get-funds:main": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/get-funds.ts --mode main",
        "get-funds:sniper": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/get-funds.ts --mode sniper",
        "get-funds:trader": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/get-funds.ts --mode trader",
        "get-funds:whale": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/get-funds.ts --mode whale",
        "get-mint": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/get-mint.ts",
        "get-pool:raydium": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/raydium/get-pool.ts",
        "generate-encryption-secret": "hexdump -vn16 -e'4/4 \"%08x\" 1 \"\n\"' /dev/urandom",
        "get-comments:twitter": "shuf -n30 data/twitter/comments.txt | bash -c 'set -a; source .env.$(yarn get-env); envsubst'",
        "grind-keypairs": "yarn grind-keypair:dev && yarn grind-keypair:dsn && yarn grind-keypair:dtr && yarn grind-keypair:dwh",
        "grind-keypair:dev": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/grind-keypair.ts --prefix De --postfix V",
        "grind-keypair:dsn": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/grind-keypair.ts --prefix Ds --postfix N",
        "grind-keypair:dtr": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/grind-keypair.ts --prefix Dt --postfix R",
        "grind-keypair:dwh": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/grind-keypair.ts --prefix Dw --postfix H",
        "rename-token-files": "NODE_ENV=$(yarn get-env) ts-node -r ./src/init.ts src/tasks/rename-token-files.ts"
    },
    "keywords": [],
    "author": "EverCoinX <2901744+evercoinx@users.noreply.github.com>",
    "license": "UNLICENSED",
    "private": true,
    "type": "commonjs",
    "engines": {
        "node": ">=22"
    },
    "packageManager": "yarn@4.9.1",
    "dependencies": {
        "@pythnetwork/client": "^2.22.1",
        "@raydium-io/raydium-sdk-v2": "^0.1.133-alpha",
        "@solana/spl-token": "^0.4.13",
        "@solana/web3.js": "^1.98.2",
        "arweave": "^1.15.7",
        "bs58": "^6.0.0",
        "chalk": "^5.4.1",
        "decimal.js": "^10.5.0",
        "dotenv": "^16.5.0",
        "flat-cache": "^6.1.9",
        "got": "^11.8.6",
        "helius-sdk": "^1.5.1",
        "image-size": "^2.0.2",
        "pinata-web3": "^0.5.4",
        "pino": "^9.6.0",
        "pino-pretty": "^13.0.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.26.0",
        "@tsconfig/node20": "^20.1.5",
        "@types/big.js": "^6.2.2",
        "@types/bn.js": "^5.1.6",
        "@types/decimal.js": "^7.4.3",
        "@types/minimist": "^1.2.5",
        "@types/node": "^22.15.17",
        "eslint": "^9.26.0",
        "eslint-config-prettier": "^10.1.3",
        "eslint-plugin-prettier": "^5.4.0",
        "husky": "^9.1.7",
        "joi": "^17.13.3",
        "prettier": "^3.5.3",
        "source-map-support": "^0.5.21",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.32.0"
    },
    "resolutions": {
        "@solana/web3.js": "^1.98.2"
    }
}
